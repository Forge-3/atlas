{
  "canisters": {
    "atlas_main": {
      "candid": "src/atlas_main/atlas_main.did",
      "package": "atlas_main",
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/atlas_main-opt.wasm.gz",
      "build": [
        "./build.sh atlas_space",
        "./build.sh atlas_main"
      ],
      "declarations": {
        "bindings": [
          "js",
          "ts"
        ]
      }
    },
    "atlas_frontend": {
      "dependencies": [
        "atlas_main",
        "ckusdc_ledger_canister",
        "ckusdc_index_canister",
        "atlas_space"
      ],
      "source": [
        "src/atlas_frontend/dist"
      ],
      "type": "assets",
      "workspace": "atlas_frontend",
      "declarations": {
        "bindings": []
      }
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "declarations": {
        "bindings": [
          "js",
          "ts"
        ]
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    },
    "atlas_space": {
      "type": "custom",
      "candid": "src/atlas_space/atlas_space.did",
      "wasm": "target/wasm32-unknown-unknown/release/atlas_space-opt.wasm.gz",
      "bindings": [
        "js",
        "ts"
      ]
    },
    "ckusdc_ledger_canister": {
      "type": "custom",
      "candid": "https://github.com/dfinity/ic/releases/download/ledger-suite-icrc-2025-06-19/ledger.did",
      "wasm": "https://github.com/dfinity/ic/releases/download/ledger-suite-icrc-2025-06-19/ic-icrc1-ledger-u256.wasm.gz",
      "remote": {
        "id": {
          "ic": "xevnm-gaaaa-aaaar-qafnq-cai"
        }
      },
      "bindings": [
        "js",
        "ts"
      ],
      "specified_id": "xevnm-gaaaa-aaaar-qafnq-cai"
    },
    "ckusdc_index_canister": {
      "type": "custom",
      "candid": "https://github.com/dfinity/ic/releases/download/ledger-suite-icrc-2025-06-19/index-ng.did",
      "wasm": "https://github.com/dfinity/ic/releases/download/ledger-suite-icrc-2025-06-19/ic-icrc1-index-ng-u256.wasm.gz",
      "remote": {
        "id": {
          "ic": "xrs4b-hiaaa-aaaar-qafoa-cai"
        }
      },
      "bindings": [
        "js",
        "ts"
      ],
      "specified_id": "xrs4b-hiaaa-aaaar-qafoa-cai"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:4943",
      "type": "ephemeral"
    }
  },
  "output_env_file": ".env",
  "version": 1
}