{
  "canisters": {
    "atlas_main": {
      "candid": "src/atlas_main/atlas_main.did",
      "package": "atlas_main",
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/atlas_main-opt.wasm.gz",
      "build": [
        "./build.sh atlas_space",
        "./build.sh atlas_main"
      ],
      "optimize": "cycles",
      "declarations": {
        "bindings": [
          "js",
          "ts"
        ]
      }
    },
    "atlas_frontend": {
      "dependencies": [
        "atlas_main"
      ],
      "source": [
        "src/atlas_frontend/dist"
      ],
      "type": "assets",
      "workspace": "atlas_frontend",
      "declarations": {
        "bindings": []
      }
    },
  
    "atlas_space": {
      "type": "custom",
      "candid": "src/atlas_space/atlas_space.did",
      "wasm": "target/wasm32-unknown-unknown/release/atlas_space-opt.wasm.gz",
      "bindings": [
        "js",
        "ts"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:4943",
      "type": "ephemeral"
    }
  },
  "output_env_file": ".env",
  "version": 1
}